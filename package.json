{
  "name": "appr2",
  "version": "1.0.0",
  "description": "My striking Nuxt.js project",
  "author": "mlengse",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "postzip": "mv functions/chrome/chrome.zip .functions",
    "prebuild": "npm run zip",
    "prezip": "rm -rf .functions && mkdir .functions",
    "zip": "cd functions && npm install && chmod -R 777 node_modules && zip -r ekin.zip *",
    "nuxt": "nuxt",
    "dev": "npm-run-all --parallel nuxt functions:serve",
    "start": "nuxt start",
    "generate": "NODE_ENV=production nuxt generate && npm run functions:build",
    "functions:serve": "netlify-lambda serve -c ./webpack.config.js functions",
    "functions:build": "netlify-lambda build functions",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv-webpack": "^1.7.0",
    "netlify-lambda": "^1.6.3",
    "nuxt": "^2.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0"
  }
}
