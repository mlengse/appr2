{
  "name": "appr2",
  "version": "1.0.0",
  "description": "My striking Nuxt.js project",
  "author": "mlengse",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "nuxt": "nuxt",
    "dev": "npm-run-all --parallel nuxt functions:serve",
    "start": "nuxt start",
    "generate": "NODE_ENV=production nuxt generate && npm run functions:build",
    "functions:serve": "netlify-lambda serve --config ./webpack.functions.js functions",
    "functions:build": "netlify-lambda build functions",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-0",
    "bufferutil": "^4.0.1",
    "chrome-aws-lambda": "^2.0.2",
    "dotenv-webpack": "^1.7.0",
    "netlify-lambda": "^1.6.3",
    "nuxt": "^2.0.0",
    "puppeteer-core": "^2.0.0",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0"
  }
}
